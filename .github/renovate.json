{
  "extends": [
    "config:recommended"
  ],
  "platform": "github",
  "configMigration": true,
  "automerge": false,
  "bumpVersion": "patch",
  "ignoreTests": true,
  "repositories": [
    "0Bu/rpi-docker-homebridge"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "(^|/)Dockerfile$"
      ],
      "matchStrings": [
        "#\\s*renovate:\\s*datasource=(?<datasource>.*?) depName=(?<depName>.*?)( versioning=(?<versioning>.*?))?\\sENV .*?_VERSION=\"?(?<currentValue>.*?)\"?\\s"
      ]
    }
  ]
}
